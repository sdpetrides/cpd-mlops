{
    "include": [
        "*.py"
    ],
    "exclude": [
        "**/__pycache__",
    ],
    "ignore": [
        "*.ipynb"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "reportMissingImports": false,
    "reportMissingTypeStubs": false,
    "reportMissingModuleSource": false,
    "pythonVersion": "3.9",
    "pythonPlatform": "Linux",
    "source": "https://github.com/microsoft/pyright/blob/main/docs/configuration.md"
}